  <scm>
    <connection>scm:git:${project.url}</connection>
    <developerConnection>scm:git:${project.url}</developerConnection>
    <tag>HEAD</tag>
    <url>${project.url}</url>
  </scm>
  <ciManagement>
    <system>jenkins</system>
    <url>https://jenkins-devops.app-dev.santalucia.net/</url>
    <notifiers>
      <notifier>
        <type>mail</type>
        <address>ignacio.lopezv@santalucia.es</address>
        <sendOnError>true</sendOnError>
        <sendOnFailure>true</sendOnFailure>
        <sendOnSuccess>false</sendOnSuccess>
        <sendOnWarning>false</sendOnWarning>
      </notifier>
    </notifiers>
  </ciManagement>
  <properties>
    <bp.jvmJlinkAddModules>java.base,java.logging,java.naming,java.desktop,jdk.unsupported,java.sql,java.management,java.security.jgss,jdk.jfr,jdk.management,java.instrument,java.rmi,jdk.crypto.ec</bp.jvmJlinkAddModules>
    <skip.it>true</skip.it>
    <spring-ai.version>1.0.1</spring-ai.version>
    <spring-shell-starter.version>3.4.0</spring-shell-starter.version>
    <surfire.output.skip>false</surfire.output.skip>
    <swagger.codegen.async.server>false</swagger.codegen.async.server>
    <swagger.codegen.inputSpec>src/main/resources/api/swagger.yaml</swagger.codegen.inputSpec>
    <swagger.codegen.package>${project.groupId}</swagger.codegen.package>
    <swagger.codegen.skip.client>true</swagger.codegen.skip.client>
    <swagger.codegen.skip.server>false</swagger.codegen.skip.server>
  </properties>
