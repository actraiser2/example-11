  <build>
    <plugins>
      <plugin>
        <groupId>org.graalvm.buildtools</groupId>
        <artifactId>native-maven-plugin</artifactId>
      </plugin>
      <plugin>
        <groupId>org.springframework.boot</groupId>
        <artifactId>spring-boot-maven-plugin</artifactId>
        <configuration>
          <jvmArguments>-Djdk.tracePinnedThreads=full -Dfile.encoding=utf-8</jvmArguments>
          <image>
            <name>docker.io/actraiser/example-11-app</name>
            <publish>true</publish>
          </image>
          <docker>
            <publishRegistry>
              <username>actraiser</username>
              <password>fenix000</password>
            </publishRegistry>
          </docker>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.jsonschema2pojo</groupId>
        <artifactId>jsonschema2pojo-maven-plugin</artifactId>
        <dependencies />
        <executions>
          <execution>
            <goals>
              <goal>generate</goal>
            </goals>
            <configuration>
              <includeGetters>false</includeGetters>
              <includeSetters>false</includeSetters>
              <includeAdditionalProperties>true</includeAdditionalProperties>
              <sourcePaths>
                <sourcePath>${project.basedir}/src/main/resources/json-schema/random_name.jsd.json</sourcePath>
              </sourcePaths>
              <targetPackage>com.santalucia.example.infrastructure.model</targetPackage>
            </configuration>
          </execution>
        </executions>
      </plugin>
      
      <plugin>
            <groupId>org.codehaus.mojo</groupId>
            <artifactId>build-helper-maven-plugin</artifactId>
            <executions>
              <execution>
                <id>add-source-jsonschema2pojo-execution-test</id>
                <goals>
                  <goal>add-source</goal>
                </goals>
                <phase>generate-sources</phase>
                <configuration>
                  <sources>
                    <source>target/generated-sources/jsonschema2pojo</source>
                  </sources>
                </configuration>
              </execution>
            </executions>
          </plugin>
