<!DOCTYPE generatorConfiguration PUBLIC
 "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
 "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
  <!-- **************************************************************************
  	** INFO: Para ejecutar en maven usar el comando:  mvn mybatis-generator:generate 
  	** el reference se encuentra en https://mybatis.org/generator/configreference/xmlconfig.html
  	************************************************************************** -->
  <!-- !!!! Configuracion del contexto INFORMIX !!!! -->
  <context id="dsqlInformix" targetRuntime="MyBatis3DynamicSql">
    

	<commentGenerator>
	  <property name="suppressDate" value="true" />
	</commentGenerator>
    
    <jdbcConnection driverClass="com.informix.jdbc.IfxDriver" connectionURL="jdbc:informix-sqli://192.168.204.226:20101/siis:INFORMIXSERVER=fun_jdbc" userId="amsdacom" password="Am5Spr1ngD4c0m"/>
    <!-- <jdbcConnection driverClass="oracle.jdbc.OracleDriver" connectionURL="jdbc:oracle:thin:@slbdorad15.servidores.net:1548/DB12UTF8D.DES.SANTALUCIA.ES" userId="LAB_ARQ_SW_DEV" password="lab_arq_sw_dev"/>-->


    <javaTypeResolver>
	  <property name="useJSR310Types" value="true" />
	</javaTypeResolver>

    <!-- !!!! Configuracion del modelo !!!! -->
    <javaModelGenerator targetPackage="com.santalucia.example.infrastructure.entity" targetProject="src/main/java">
    	<!-- util para informix ya que los campos son CHAR; retirar para oracle ya que son varchar -->
    	<!-- si se quiere de forma especifica se puede habilitar a nivel de tabla, no de modelo -->
    	<property name="trimStrings" value="true" /> 
    	<!-- si se quiere o no generar un subpaquete por schema 
    	<property name="enableSubPackages" value="false"/>
    	-->
    </javaModelGenerator>
    
    <!-- !!!! Configuracion del modelo !!!! -->
    <!-- WARNING: Cambiar el package a primary o secondary dependiendo de si es informix o oracle; depende de cada app -->
    <javaClientGenerator targetPackage="com.santalucia.example.infrastructure.mybatis.primary" targetProject="src/main/java"/>
      
     <!-- !!!! Tablas a exportar !!!! -->
    <table tableName="cacetrafec">
    	<!-- WARNING: Descomentar si la tabla contiene campos de tipo LVARCHAR; Solo aplicable a Informix 
    	<columnOverride column="LONG_VARCHAR_FIELD" jdbcType="VARCHAR" javaType="java.lang.String" />-->
    </table>
    
  </context>
  
  <!-- !!!! Configuracion del contexto ORACLE !!!! -->
  <context id="dsqlOracle" targetRuntime="MyBatis3DynamicSql">
  
  	<commentGenerator>
	  <property name="suppressDate" value="true" />
	</commentGenerator>
    
    <jdbcConnection driverClass="oracle.jdbc.OracleDriver" connectionURL="jdbc:oracle:thin:@slbdorad15.servidores.net:1548/DB12UTF8D.DES.SANTALUCIA.ES" userId="LAB_ARQ_SW_DEV" password="lab_arq_sw_dev"/>

    <javaTypeResolver>
	  <property name="useJSR310Types" value="true" />
	</javaTypeResolver>
	
    <!-- !!!! Configuracion del modelo !!!! -->
    <javaModelGenerator targetPackage="com.santalucia.example.infrastructure.entity" targetProject="src/main/java">
    	<!-- util para informix ya que los campos son CHAR; retirar para oracle ya que son varchar -->
    	<!-- si se quiere de forma especifica se puede habilitar a nivel de tabla, no de modelo -->
    	<property name="trimStrings" value="true" /> 
    	<!-- si se quiere o no generar un subpaquete por schema 
    	<property name="enableSubPackages" value="false"/>
    	-->
    </javaModelGenerator>
    
    <!-- !!!! Configuracion del modelo !!!! -->
    <!-- WARNING: Cambiar el package a primary o secondary dependiendo de si es informix o oracle; depende de cada app -->
    <javaClientGenerator targetPackage="com.santalucia.example.infrastructure.mybatis.secondary" targetProject="src/main/java"/>
        
     <!-- !!!! Tablas a exportar !!!! -->
    <table tableName="employee" />
    
  </context>
